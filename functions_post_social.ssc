f.post_social_update{
  v.content=v.function_passed[0]

  if set v.content
    v.compressed=s.base64_encode(v.content)
    v.datecode=s.timestamp()
    v.postid=s.mysql_insert("INSERT INTO socials_updates(user,date) VALUES('sv.user_id','v.datecode')")
    if not v.postid false
      v.postid=s.mysql_insert("INSERT INTO socials_updates_contents(update,content) VALUES('v.postid','v.compressed')")
    end
  end
}
